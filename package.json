{
  "name": "thread-promises",
  "type": "commonjs",
  "version": "1.6.1",
  "description": "Promises running in separated threads",
  "source": "index.ts",
  "targets": {
    "cdn-lib": {
      "source": "browsers.ts",
      "outputFormat": "global"
    },
    "module": {
      "source": "index.ts",
      "context": "browser",
      "outputFormat": "commonjs"
    },
    "types": {
      "source": "index.ts",
      "context": "browser",
      "outputFormat": "commonjs"
    }
  },
  "lib": "es2015",
  "context": "browser",
  "cdn-lib": "lib/thread-promises.min.js",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/types.d.ts",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshshe/ThreadPromises.git"
  },
  "keywords": [
    "promise",
    "threads",
    "multithreading",
    "webworkers"
  ],
  "author": "kshshe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kshshe/ThreadPromises/issues"
  },
  "homepage": "https://github.com/kshshe/ThreadPromises#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.0",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@types/node": "^18.11.10",
    "parcel": "^2.8.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
